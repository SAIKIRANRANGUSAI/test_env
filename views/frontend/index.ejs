    <!-- faq start -->
    <section class="section faq-section" id="faqa">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-12 col-lg-7 col-xxl-6">
        <div class="section__header">
          <span class="section__header-sub-title headingFour wow fadeInDown" data-wow-duration="0.8s">
            <%= faqSection.section_subtitle %>
          </span>
          <h2 class="section__header-title wow fadeInUp" data-wow-duration="0.8s">
            <%= faqSection.section_title %>
          </h2>
          <p class="section__header-content wow fadeInDown" data-wow-duration="0.8s">
            <%= faqSection.section_description %>
          </p>
        </div>
      </div>
    </div>

    <div class="row justify-content-between gy-4 gy-lg-0">
      <% 
        // Split FAQs into two halves for left/right columns
        const half = Math.ceil(faqs.length / 2);
        const leftFaqs = faqs.slice(0, half);
        const rightFaqs = faqs.slice(half);
      %>

      <!-- Left Column -->
      <div class="col-12 col-lg-6 col-xxl-6">
        <div class="accordion wow fadeInDown" data-wow-duration="0.8s" id="faq">
          <% leftFaqs.forEach((faq, index) => { %>
            <div class="accordion-item <%= index === 0 ? 'accordion_bg' : '' %>">
              <h5 class="accordion-header">
                <button class="accordion-button <%= index !== 0 ? 'collapsed' : '' %>" type="button"
                  data-bs-toggle="collapse" data-bs-target="#faq-accordion-<%= index + 1 %>"
                  aria-expanded="<%= index === 0 ? 'true' : 'false' %>"
                  aria-controls="faq-accordion-<%= index + 1 %>">
                  <%= faq.question %>
                </button>
              </h5>
              <div id="faq-accordion-<%= index + 1 %>" class="accordion-collapse collapse <%= index === 0 ? 'show' : '' %>"
                data-bs-parent="#faq">
                <div class="accordion-body">
                  <p class="mb-0"><%= faq.answer %></p>
                </div>
              </div>
            </div>
          <% }) %>
        </div>
      </div>

      <!-- Right Column -->
      <div class="col-12 col-lg-6 col-xxl-6">
        <div class="accordion wow fadeInUp" data-wow-duration="0.8s" id="faq2">
          <% rightFaqs.forEach((faq, index) => { %>
            <div class="accordion-item <%= index === 0 ? 'accordion_bg' : '' %>">
              <h5 class="accordion-header">
                <button class="accordion-button <%= index !== 0 ? 'collapsed' : '' %>" type="button"
                  data-bs-toggle="collapse" data-bs-target="#faq2-accordion-<%= index + 1 %>"
                  aria-expanded="<%= index === 0 ? 'true' : 'false' %>"
                  aria-controls="faq2-accordion-<%= index + 1 %>">
                  <%= faq.question %>
                </button>
              </h5>
              <div id="faq2-accordion-<%= index + 1 %>" class="accordion-collapse collapse <%= index === 0 ? 'show' : '' %>"
                data-bs-parent="#faq2">
                <div class="accordion-body">
                  <p class="mb-0"><%= faq.answer %></p>
                </div>
              </div>
            </div>
          <% }) %>
        </div>
      </div>

    </div>
  </div>
</section>

    <!-- faq end -->

