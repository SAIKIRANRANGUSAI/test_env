<%- include('./partials/header') %>
    <!-- Hero Section Start -->
    <section class="hero">
      <div class="hero__animation">
        <img src="/images/hero_vector_dollar.png" alt="Image" />
        <img src="/images/hero_vector_message.png" alt="Image" />
        <img src="/images/hero_vector_dollar.png" alt="Image" />
        <img src="/images/hero_vector_setting.png" alt="Image" />
        <img src="/images/hero_vector_arrow.png" alt="Image" />
      </div>
      <div class="container">
        <div
          class="row gy-5 gy-lg-0 align-items-center justify-content-between"
        >
          <div class="col-12 col-lg-6">
            <div class="section__content">
              <span
                class="section__content-sub-title headingFour wow fadeInDown"
                data-wow-duration="0.8s"
              >
                My Loan Bazaar</span
              >
              <h1 class="section__content-title display-3 wow fadeInUp" data-wow-duration="0.8s">
  <% 
    const headerText = hero.hero_header || "Find the Perfect Loan for Your Needs"; 
    // Split header into words
    const words = headerText.split(" ");
    const staticStart = words[0] + " " + words[1]; // "Find the"
    const animatedWord = words[2]; // "Perfect"
    const staticEnd = words.slice(3).join(" "); // "Loan for Your Needs"
  %>

  <%= staticStart %>
  <span class="word d-inline-flex">
    <% animatedWord.split("").forEach((letter, index) => { %>
      <span 
        data-wow-duration="0.6s" 
        data-wow-delay="<%= (index * 0.1).toFixed(1) %>s"
        class="letter wow fadeInDown"
      ><%= letter %></span>
    <% }) %>
  </span>
  <%= " " + staticEnd %>
</h1>

              <p
                class="section__content-text wow fadeInDown"
                data-wow-duration="0.8s"
              >
                <%= hero.hero_description || "Default hero description goes here." %>
              </p>
              <div class="btn-group mt_40 wow fadeInUp" data-wow-duration="0.8s">
                <a href="/contact" class="btn_theme btn_theme_active"
                  >Contact Us<i class="bi bi-arrow-up-right"></i><span></span
                ></a>
                <a href="/about" class="btn_theme"
                  >About us<i class="bi bi-arrow-up-right"></i><span></span
                ></a>
              </div>
            </div>
          </div>
          <div class="col-12 col-lg-6 col-xxl-5">
            <div class="hero__thumb wow fadeInUp" data-wow-duration="0.8s">
              <img src="<%= hero.hero_image || '/images/default-hero.jpg' %>" alt="Hero Image">
            </div>
          </div>
        </div>
      </div>
    </section>
    <!--Hero Section End -->

    <!-- feature start -->
    <section class="feature section">
      <div class="animation">
        <img src="/images/hero_vector_dollar.png" alt="Image" />
        <img src="/images/hero_vector_message.png" alt="Image" />
      </div>
      <div class="container">
        <div class="row justify-content-center">
          <div class="col-12 col-lg-9 col-xxl-8">
            <div class="section__header">
              <span
                class="section__header-sub-title headingFour wow fadeInDown"
                data-wow-duration="0.8s"
              >
                Featured Services</span
              >
              <h2 class="section__header-title wow fadeInUp" data-wow-duration="0.8s">
  <%= hero.services_heading || "Empowering You with Loan Knowledge and Comparison testimonial" %>
</h2>
<p class="section__header-content wow fadeInDown" data-wow-duration="0.8s">
  <%= hero.services_description || "We are dedicated to providing you with valuable services that simplify your loan search and empower you to make informed borrowing decisions. Our comprehensive range of services" %>
</p>

            </div>
          </div>
        </div>
        <div class="row justify-content-center">
  <% if (allServices && allServices.length > 0) { %>
    <% allServices.forEach((service) => { %>
      <div class="col-md-3 mb-3">
        <div class="card card--custom">
          <div class="card__icon">
            <img src="<%= service.icon_image %>" width="50" alt="" />
          </div>
          <div class="card__content">
            <h4 class="card__title">
              <a href="/personal-loan/<%= service.id %>"><%= service.heading %></a>
            </h4>
            <p class="fs-small">
              <%= service.short_description %>
            </p>
            <a href="/personal-loan/<%= service.id %>" class="btn_theme social_box">
              <i class="bi bi-arrow-up-right"></i><span></span>
            </a>
          </div>
        </div>
      </div>
    <% }); %>
  <% } else { %>
    <p>No services found.</p>
  <% } %>
</div>

        <!--row-->
      </div>
    </section>
    <!-- feature end -->

    <!-- About Us start -->
    <section class="about-us section">
      <div class="container">
        <div
          class="row gy-5 gy-lg-0 justify-content-between align-items-center"
